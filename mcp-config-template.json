{
  "mcpServers": {
    "mtender": {
      "command": "node",
      "args": ["/path/to/mtender-server/build/index.js"],
      "disabled": false,
      "alwaysAllow": []
    },
    "mtender-docker": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "-v", "/path/to/logs:/app/logs", "mtender-mcp-server"],
      "disabled": false,
      "alwaysAllow": []
    }
  }
}